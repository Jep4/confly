            <td> number </td>
            <td> writer </td>
            <td> title </td>
            <td> views </td>
            <td> modified date </td>
            <td> posted date </td>
        </tr>
    <% for(var i = 0; i<rows.length; i++){ 
    var data=rows[i];%>
